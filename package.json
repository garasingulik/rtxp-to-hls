{
  "name": "rtxp-to-hls",
  "version": "0.0.1",
  "description": "RTxP to HLS Realtime Stream Converter",
  "main": "build/src/server.js",
  "types": "build/src/server.d.ts",
  "files": [
    "build/src"
  ],
  "scripts": {
    "test": "ava --timeout=30s --verbose",
    "pretest": "eslint **/*.ts",
    "build": "tsc -p .",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "start": "node build/src/server.js",
    "dev": "TS_NODE_FILES=true ts-node src/server.ts",
    "lint": "eslint **/*.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garasingulik/rtxp-to-hls.git"
  },
  "keywords": [
    "typescript",
    "express",
    "template",
    "bootstrap",
    "skeleton"
  ],
  "author": "Indra Gunawan <indra@garasingulik.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/garasingulik/rtxp-to-hls/issues"
  },
  "homepage": "https://github.com/garasingulik/rtxp-to-hls#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fp-ts": "^2.9.3",
    "io-ts": "^2.2.13",
    "io-ts-reporters": "^1.2.2",
    "moment": "^2.29.1",
    "tswrap": "^1.0.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/moment": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.14.0",
    "ava": "^3.15.0",
    "eslint": "^7.18.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "ava": {
    "files": [
      "./test/**/*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
